  file = {/Users/NPK/Zotero/storage/TAC9W243/Fairfax et al. - 2012 - GENETICS OF GENE EXPRESSION IN PRIMARY IMMUNE CELL.pdf},
  journal = {Nature genetics},
  number = {5},
  pmcid = {PMC3437404},
  pmid = {22446964}
}

