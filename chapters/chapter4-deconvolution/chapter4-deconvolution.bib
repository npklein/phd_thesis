  author = {Willemsen, Gonneke and de Geus, Eco J. C. and Bartels, Meike and van Beijsterveldt, C. E. M. Toos and Brooks, Andy I. and Burk, G. Frederique Estourgie-van and Fugman, Douglas A. and Hoekstra, Chantal and Hottenga, Jouke-Jan and Kluft, Kees and Meijer, Piet and Montgomery, Grant W. and Rizzu, Patrizia and Sondervan, David and Smit, August B. and Spijker, Sabine and Suchiman, H. Eka D. and Tischfield, Jay A. and Lehner, Thomas and Slagboom, P. Eline and Boomsma, Dorret I.},
  year = {2010},
